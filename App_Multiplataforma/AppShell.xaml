<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="App_Multiplataforma.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:App_Multiplataforma"
    Shell.FlyoutBehavior="Flyout"
    Title="App_Multiplataforma">

    <ShellContent
        Title="Inicio"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
    Title="Secundaria"
    ContentTemplate="{DataTemplate local:secundaria}"
    Route="secundaria" />

</Shell>
